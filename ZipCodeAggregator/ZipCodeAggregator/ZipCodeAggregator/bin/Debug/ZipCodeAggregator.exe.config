<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="USZipSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="USZipSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://www.webservicex.net/uszip.asmx" binding="basicHttpBinding"
                bindingConfiguration="USZipSoap" contract="ServiceReferenceZipSoap.USZipSoap"
                name="USZipSoap" />
            <endpoint address="http://www.webservicex.net/uszip.asmx" binding="customBinding"
                bindingConfiguration="USZipSoap12" contract="ServiceReferenceZipSoap.USZipSoap"
                name="USZipSoap12" />
        </client>
    </system.serviceModel>
</configuration>